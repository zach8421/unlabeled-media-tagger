# Environment Variables for unlabeled-media-tagger
# Copy this file to .env and fill in your values

# Google Drive API Credentials
# GOOGLE_DRIVE_CREDENTIALS_PATH=credentials.json
# GOOGLE_DRIVE_TOKEN_PATH=token.json
# GOOGLE_DRIVE_FOLDER_ID=your_folder_id_here

# Model Configuration
# MODEL_DEVICE=cpu  # Options: cpu, cuda, mps
# FACE_DETECTION_MODEL=models/face_detection.pth
# FACE_RECOGNITION_MODEL=models/face_recognition.pth
# OBJECT_DETECTION_MODEL=models/object_detection.pth

# Pipeline Settings
# BATCH_SIZE=10
# FRAME_INTERVAL=1.0
# MAX_FRAMES=100
# OUTPUT_DIR=./output

# Logging
# LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR
